<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.2//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.penske.apps.adminconsole.dao.DistrictProximityDao">

 	<!-- To Load the Details in District Proximity Configuration Page-->
	<select  id="getDistrictProximity" resultType="com.penske.apps.adminconsole.model.DistrictProximity">
		SELECT   DISTRICT AS district,
		         TIER AS tier
		FROM     SMC.OBM_PLANT_PROXIMITY OPP
		WHERE    OPP.TIER = 1
	</select>
	
	<insert id="insertProximityValues">
	</insert>
</mapper>